@model IEnumerable<ContosoUniversity.Models.SchoolViewModels.EnrollmentDateGroup>
@{
    ViewData["Title"] = "About";
}

<h2>Student Bbody Statistics</h2>

<table>
    <tr>
        <th>@Html.DisplayNameFor(model =>model.EnrollmentDate)</th>
        <th>@Html.DisplayNameFor(model =>model.StudentCount)</th>
    </tr>
    @foreach (var item in Model)
    { 
        <tr>
            <td>@Html.DisplayFor(modelItem =>item.EnrollmentDate)</td>
            <td>@Html.DisplayFor(modelItem =>item.StudentCount)</td>
        </tr>
    }
</table>

